[sub_resource type="GDScript" id="GDScript_7wx6e"]
script/source = "@tool
extends LineEdit
@onready var clear_button: Button = $HBoxContainer/SearchClear


func _ready() -> void:
	clear_button.icon = EditorInterface.get_editor_theme().get_icon(\"GuiClose\", \"EditorIcons\")
	$HBoxContainer/Search.icon = EditorInterface.get_editor_theme().get_icon(\"Search\", \"EditorIcons\")

func _on_text_changed(new_text: String) -> void:
	if new_text.is_empty():
		clear_button.hide()
	else:
		clear_button.show()

func _on_search_clear_pressed() -> void:
	text = String()
	text_changed.emit(text)
"

